@model IEnumerable<HabitatForHumanity.Models.User.Demog>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ageBracket)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numPeople)
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ageBracket)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numPeople)
            </td>
            
        </tr>
    }

</table>
